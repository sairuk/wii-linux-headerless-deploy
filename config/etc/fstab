/dev/mmcblk0p2  /      ext4   noatime,errors=remount-ro                         0 1
/dev/mmcblk0p1  /boot  vfat   ro,noatime,nodiratime,errors=remount-ro           0 2
/dev/mmcblk0p3  swap   swap   defaults	                                        0 0
tmpfs           /tmp   tmpfs  defaults,noatime,nosuid,nodev,size=40M,mode=1777  0 0

